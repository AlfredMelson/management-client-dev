{
  "name": "management-client-dev",
  "version": "1.0.0",
  "license": "MIT",
  "tags": [
    "employee-management"
  ],
  "keywords": [
    "trpc",
    "framer-motion",
    "react-query",
    "react",
    "react-router-dom",
    "recoil",
    "tailwinds",
    "typescript",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfredMelson/management-client-dev.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "start-tailwind": "npx tailwindcss -i ./src/style/input.css -o ./public/output.css --watch",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,css,md,json}\"",
    "lint": "eslint -f unix \"src/**/*.{js,ts,tsx}\"",
    "typecheck": "tsc --pretty",
    "fullcheck": "yarn format && yarn lint && yarn typecheck",
    "clean": "lerna clean --yes && rimraf node_modules yarn.lock"
  },
  "dependencies": {
    "@trpc/client": "9.22.0",
    "@trpc/react": "9.22.0",
    "@trpc/server": "9.22.0",
    "framer-motion": "6.3.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-query": "3.38.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "0.7.2",
    "uuid": "8.3.2",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.0",
    "@types/eslint": "8.4.1",
    "@types/eslint-config-prettier": "6.11.0",
    "@types/node": "17.0.25",
    "@types/prettier": "2.6.0",
    "@types/react": "~18.0.6",
    "@types/react-dom": "~18.0.2",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "@types/uuid": "8.3.4",
    "autoprefixer": "10.4.4",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "lerna": "4.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "rimraf": "3.0.2",
    "tailwindcss": "3.0.24",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

